{
  "name": "sophon-ui",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:int-test": "ng serve --aot=true --configuration=int-test",
    "start:proxy": "ng serve --aot=false --proxy-config proxy.conf.json",
    "start:host": "ng serve --open --host admin-ui.mpl --port 4200 --disable-host-check",
    "build": "ng build",
    "build:prod": "ng build --prod=true --aot=true --extractCss=true --optimization=true",
    "build:prod:profile": "ng build --prod --aot --profile",
    "test": "ng test",
    "test:cypress:record": "cypress run --record --key 90cc1229-d7c3-417b-8bab-db33914607ee",
    "cypress:int-test": "yarn run cypress open",
    "cypress:local": "yarn run cypress open --config-file=cypress.local.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:ci": "ng test --no-watch --no-progress --browsers ChromeHeadlessCI",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.1",
